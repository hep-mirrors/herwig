// -*- C++ -*-
//
// This is the implementation of the inlined member functions of
// the Remnant class.
//

namespace Herwig {
using namespace ThePEG;

inline Remnant::Remnant() 
  : Particle(CurrentGenerator::current().getParticleData(ExtraParticleID::Remnant))
{}

inline void * Remnant::operator new(size_t) {
  return FixedAllocator<Remnant>::allocate();
}

inline void Remnant::operator delete(void * p, size_t) {
  FixedAllocator<Remnant>::deallocate(p);
}

}
