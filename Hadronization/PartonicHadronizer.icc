namespace Herwig {

inline PartonicHadronizer::PartonicHadronizer() 
  : _exclusive(true),_partontries(10),_inter(false) {}

inline IBPtr PartonicHadronizer::clone() const {
  return new_ptr(*this);
}

inline IBPtr PartonicHadronizer::fullclone() const {
  return new_ptr(*this);
}

