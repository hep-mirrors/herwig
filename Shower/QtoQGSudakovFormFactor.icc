namespace Herwig {


  inline QtoQGSudakovFormFactor::QtoQGSudakovFormFactor() 
    : SudakovFormFactor() {}


  inline QtoQGSudakovFormFactor::QtoQGSudakovFormFactor(const QtoQGSudakovFormFactor & x)
    : SudakovFormFactor(x) {}


  inline QtoQGSudakovFormFactor::
  QtoQGSudakovFormFactor( const SplitFunPtr inputSplitFun, 
			  const tShowerAlphaPtr inputShowerAlpha,
			  const Energy inputInfScale, const Energy inputSupScale, 
			  const Energy inQ0 ) :
    SudakovFormFactor(inputSplitFun, inputShowerAlpha, inputInfScale, inputSupScale, inQ0) {}
  

