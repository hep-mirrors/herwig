# set /Herwig/Particles/pbar-:PDF MRST2
cd /Herwig/MatrixElements/
insert SimpleDIS:MatrixElements[0] MEDISNC
cd /Herwig/Generators
create Herwig::DISTest NeutralTest DISTest.so
insert DISGenerator:AnalysisHandlers 0 NeutralTest
saverun DIS-Neutral DISGenerator
