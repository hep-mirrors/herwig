set EventGenerator:PrintEvent 1000
set EventGenerator:MaxErrors 10000
set /Herwig/Cuts/NeutralCurrentCut:MaxQ2 1000000.
set /Herwig/Cuts/NeutralCurrentCut:MinW2 0.
set /Herwig/Cuts/NeutralCurrentCut:MaxW2 1000000.
set /Herwig/Cuts/ChargedCurrentCut:MaxQ2 1000000.
set /Herwig/Cuts/ChargedCurrentCut:MinW2 0.
set /Herwig/Cuts/ChargedCurrentCut:MaxW2 1000000.
set /Herwig/Cuts/Cuts:MHatMin 0.001
set /Herwig/Cuts/Cuts:X1Min 1e-10
set /Herwig/Cuts/Cuts:X2Min 1e-10
set /Herwig/Particles/e+:PDF /Herwig/Partons/NoPDF
set /Herwig/Particles/e-:PDF /Herwig/Partons/NoPDF
