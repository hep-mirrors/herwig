//
// This is the implementation of the inlined member functions of
// the QtoQGSplitFn class.
//

namespace Herwig {

inline QtoQGSplitFn::QtoQGSplitFn() : SplittingFunction(ShowerIndex::QCD,1)
{}

inline QtoQGSplitFn::QtoQGSplitFn(const QtoQGSplitFn & x)
  : SplittingFunction(x) {}

inline IBPtr QtoQGSplitFn::clone() const {
  return new_ptr(*this);
}

inline IBPtr QtoQGSplitFn::fullclone() const {
  return new_ptr(*this);
}

}
