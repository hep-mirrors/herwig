//
// This is the implementation of the inlined member functions of
// the MEPP2Higgs class.
//

namespace Herwig {

inline MEPP2Higgs::MEPP2Higgs() : 
  widthopt(1),
  branchingopt(3),
  shapeopt(1),
  processopt(1),
  minflavouropt(3),
  maxflavouropt(5)
{}

inline IBPtr MEPP2Higgs::clone() const {
  return new_ptr(*this);
}

inline IBPtr MEPP2Higgs::fullclone() const {
  return new_ptr(*this);
}

MEPP2Higgs::~MEPP2Higgs() {}

}
