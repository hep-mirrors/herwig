namespace Herwig {

inline MEPP2Higgs::MEPP2Higgs() : 
  shapeopt(1),
  processopt(1),
  minflavouropt(4),
  maxflavouropt(5)
{}

inline IBPtr MEPP2Higgs::clone() const {
  return new_ptr(*this);
}

inline IBPtr MEPP2Higgs::fullclone() const {
  return new_ptr(*this);
}
}
