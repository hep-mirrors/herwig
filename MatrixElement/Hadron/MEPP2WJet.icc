//
// This is the implementation of the inlined member functions of
// the MEPP2WJet class.
//

namespace Herwig {

inline MEPP2WJet::MEPP2WJet() : _process(0), _maxflavour(5), _plusminus(0),
				_wdec(0), _widthopt(1)
{}

inline IBPtr MEPP2WJet::clone() const {
  return new_ptr(*this);
}

inline IBPtr MEPP2WJet::fullclone() const {
  return new_ptr(*this);
}

}
