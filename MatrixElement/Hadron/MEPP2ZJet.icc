//
// This is the implementation of the inlined member functions of
// the MEPP2ZJet class.
//

namespace Herwig {

inline MEPP2ZJet::MEPP2ZJet() : _process(0), _maxflavour(5), _zdec(0),
				_gammaZ(0)
{}

inline IBPtr MEPP2ZJet::clone() const {
  return new_ptr(*this);
}

inline IBPtr MEPP2ZJet::fullclone() const {
  return new_ptr(*this);
}

}
