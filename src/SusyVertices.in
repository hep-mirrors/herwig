# create the W scalar-scalar vertex
create Herwig::SSWSSVertex WSFSFVertex
set /Defaults/Model:Vertex/WSFSF WSFSFVertex

# create the neutralino-fermion-scalar vertex
create Herwig::SSNFSVertex NFSVertex
set /Defaults/Model:Vertex/NFSF NFSVertex

# create the gluino-fermion-scalar vertex
create Herwig::SSGFSVertex GFSVertex
set /Defaults/Model:Vertex/GFSF GFSVertex

# create the first Higgs-scalar-scalar vertex
create Herwig::SSHSFSFVertex HSSVertex
set /Defaults/Model:Vertex/HSFSF HSSVertex

# create the chargino-fermion-scalar vertex
create Herwig::SSCFSVertex CFSVertex
set /Defaults/Model:Vertex/CFSF CFSVertex

# create gluon-scalar-scalar vertex
create Herwig::SSGSSVertex GSSVertex
set /Defaults/Model:Vertex/GSFSF GSSVertex

# create 4point gluon-squark vertex
create Herwig::SSGGSQSQVertex GGSQSQVertex
set /Defaults/Model:Vertex/GGSQSQ GGSQSQVertex

# create gluon-gluino-gluino vertex
create Herwig::SSGSGSGVertex GSGSGVertex
set /Defaults/Model:Vertex/GSGSG GSGSGVertex

# create neutralino-neutralino-Z vertex
create Herwig::SSNNZVertex NNZVertex
set /Defaults/Model:Vertex/NNZ NNZVertex

# create chargino-chargino-Z vertex 
create Herwig::SSCCZVertex CCZVertex
set /Defaults/Model:Vertex/CCZ CCZVertex

# create chargino-neutralino-W vertex
create Herwig::SSCNWVertex CNWVertex
set /Defaults/Model:Vertex/CNW CNWVertex

# MSSM fermion-higgs vertex
create Herwig::SSFFHVertex SSFFHVertex
set /Defaults/Model:Vertex/SSFFH SSFFHVertex

# MSSM gaugino-gaugino-higgs vertex
create Herwig::SSGOGOHVertex GOGOHVertex
set /Defaults/Model:Vertex/GOGOH GOGOHVertex

# MSSM gauge-boson pair with higgs 
create Herwig::SSWWHVertex SSWWHVertex
set /Defaults/Model:Vertex/SSWWH SSWWHVertex

# MSSM gauge-boson with a pair of higgs 
create Herwig::SSWHHVertex SSWHHVertex
set /Defaults/Model:Vertex/SSWHH SSWHHVertex

# MSSM higgs self coupling
create Herwig::SSHHHVertex SSHHHVertex
set /Defaults/Model:Vertex/HHH SSHHHVertex

# MSSM higgs gluon effective vertex
create Herwig::SSHGGVertex SSHGGVertex
set /Defaults/Model:Vertex/SSHGG SSHGGVertex
