#################################################
# Setup of Randall-Sundrum model vertices.
#
# End users won't find anything interesting here.
#################################################

create Herwig::RSModelFFGRVertex FFGRVertex
set /Defaults/Model:Vertex/FFGR FFGRVertex
set FFGRVertex:Model /Defaults/Model

create Herwig::RSModelFFVGRVertex FFVGRVertex
set /Defaults/Model:Vertex/FFVGR FFVGRVertex
set FFVGRVertex:Model /Defaults/Model

create Herwig::RSModelSSGRVertex SSGRVertex
set /Defaults/Model:Vertex/SSGR SSGRVertex
set SSGRVertex:Model /Defaults/Model

create Herwig::RSModelVVGRVertex VVGRVertex
set /Defaults/Model:Vertex/VVGR VVGRVertex
set VVGRVertex:Model /Defaults/Model

create Herwig::RSModelVVVGRVertex VVVGRVertex
set /Defaults/Model:Vertex/VVVGR VVVGRVertex
set VVVGRVertex:Model /Defaults/Model
