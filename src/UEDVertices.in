library HwUEDVertex.so

# level-1 fermions with the SM Z
create /Herwig++/UEDF1F1Z0Vertex /Defaults/Vertices/UEDFFZ
set /Defaults/Model:Vertex/F1F1Z /Defaults/Vertices/UEDFFZ

# level-1 fermions with the SM photon
create /Herwig++/UEDF1F1P0Vertex /Defaults/Vertices/UEDFFP
set /Defaults/Model:Vertex/F1F1P /Defaults/Vertices/UEDFFP

# level-1 fermions with the SM W
create /Herwig++/UEDF1F1W0Vertex /Defaults/Vertices/UEDFFW
set /Defaults/Model:Vertex/F1F1W /Defaults/Vertices/UEDFFW

# level-1 fermions with the SM gluon
create /Herwig++/UEDF1F1G0Vertex /Defaults/Vertices/UEDF1F1G0
set /Defaults/Model:Vertex/F1F1G0 /Defaults/Vertices/UEDF1F1G0

# level-1 fermion with an SM fermion and a level-1 gluon
create /Herwig++/UEDF1F0G1Vertex /Defaults/Vertices/UEDF1F0G1
set /Defaults/Model:Vertex/F1F0G1 /Defaults/Vertices/UEDF1F0G1

# level-1 fermion with an SM fermion and a level-1 photon
create /Herwig++/UEDF1F0P1Vertex /Defaults/Vertices/UEDF1F0P1
set /Defaults/Model:Vertex/F1F0P1 /Defaults/Vertices/UEDF1F0P1

# level-1 fermion with an SM fermion and a level-1 Z
create /Herwig++/UEDF1F0Z1Vertex /Defaults/Vertices/UEDF1F0Z1
set /Defaults/Model:Vertex/F1F0Z1 /Defaults/Vertices/UEDF1F0Z1

# level-1 fermion with an SM fermion and a level-1 W
create /Herwig++/UEDF1F0W1Vertex /Defaults/Vertices/UEDF1F0W1
set /Defaults/Model:Vertex/F1F0W1 /Defaults/Vertices/UEDF1F0W1

# Pair of level-1 gluons with an SM gluon  
create /Herwig++/UEDG1G1G0Vertex /Defaults/Vertices/UEDG1G1G0
set /Defaults/Model:Vertex/G1G1G0 /Defaults/Vertices/UEDG1G1G0

# Pair of level-1 gluons with a pair of SM gluons
create /Herwig++/UEDG0G0G1G1Vertex /Defaults/Vertices/UEDG0G0G1G1
set /Defaults/Model:Vertex/G0G0G1G1 /Defaults/Vertices/UEDG0G0G1G1

# A SM photon with a pair of level-1 charged Higgs
create /Herwig++/UEDP0H1H1Vertex /Defaults/Vertices/UEDP0H1H1
set /Defaults/Model:Vertex/P0H1H1 /Defaults/Vertices/UEDP0H1H1

# A SM Z with a pair of level-1 charged Higgs
create /Herwig++/UEDZ0H1H1Vertex /Defaults/Vertices/UEDZ0H1H1
set /Defaults/Model:Vertex/Z0H1H1 /Defaults/Vertices/UEDZ0H1H1

# A SM W with a neutral high mass level-1 Higgs and a charged Higgs 
create /Herwig++/UEDW0A1H1Vertex /Defaults/Vertices/UEDW0A1H1
set /Defaults/Model:Vertex/W0A1H1 /Defaults/Vertices/UEDW0A1H1

# A SM Z with a neutral high mass level-1 Higgs and a neutral lower mass Higgs 
create /Herwig++/UEDZ0A1h1Vertex /Defaults/Vertices/UEDZ0A1h1
set /Defaults/Model:Vertex/Z0A1h1 /Defaults/Vertices/UEDZ0A1h1

# A SM W with 2 level-1 W bosons
create /Herwig++/UEDW0W1W1Vertex /Defaults/Vertices/UEDW0W1W1
set /Defaults/Model:Vertex/W0W1W1 /Defaults/Vertices/UEDW0W1W1
