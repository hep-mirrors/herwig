#
# file containing the particle data for the quarks
#
create /ThePEG/ConstituentParticleData /Defaults/Particles/d
setup /Defaults/Particles/d 1 d 0.005 0.  0 0 -1 3 2 1  0.325
defaultparticle /Defaults/Particles/d
create /ThePEG/ConstituentParticleData /Defaults/Particles/dbar
setup /Defaults/Particles/dbar -1 dbar 0.005 0.  0 0 1 -3 2 1  0.325
defaultparticle /Defaults/Particles/dbar
makeanti /Defaults/Particles/dbar /Defaults/Particles/d
create /ThePEG/ConstituentParticleData /Defaults/Particles/ubar
setup /Defaults/Particles/ubar -2 ubar 0.0023 0.  0 0 -2 -3 2 1  0.325
defaultparticle /Defaults/Particles/ubar
create /ThePEG/ConstituentParticleData /Defaults/Particles/u
setup /Defaults/Particles/u 2 u 0.0023 0.  0 0 2 3 2 1  0.325
defaultparticle /Defaults/Particles/u
makeanti /Defaults/Particles/u /Defaults/Particles/ubar
create /ThePEG/ConstituentParticleData /Defaults/Particles/c
setup /Defaults/Particles/c 4 c 1.25 0.  0 0 2 3 2 1  1.6
defaultparticle /Defaults/Particles/c
create /ThePEG/ConstituentParticleData /Defaults/Particles/cbar
setup /Defaults/Particles/cbar -4 cbar 1.25 0.  0 0 -2 -3 2 1  1.6
defaultparticle /Defaults/Particles/cbar
makeanti /Defaults/Particles/cbar /Defaults/Particles/c
create /ThePEG/ConstituentParticleData /Defaults/Particles/sbar
setup /Defaults/Particles/sbar -3 sbar 0.095 0.  0 0 1 -3 2 1  0.5
defaultparticle /Defaults/Particles/sbar
create /ThePEG/ConstituentParticleData /Defaults/Particles/s
setup /Defaults/Particles/s 3 s 0.095 0.  0 0 -1 3 2 1  0.5
defaultparticle /Defaults/Particles/s
makeanti /Defaults/Particles/s /Defaults/Particles/sbar
create /ThePEG/ConstituentParticleData /Defaults/Particles/b
setup /Defaults/Particles/b 5 b 4.2 0.  0 0 -1 3 2 1  5
defaultparticle /Defaults/Particles/b
create /ThePEG/ConstituentParticleData /Defaults/Particles/bbar
setup /Defaults/Particles/bbar -5 bbar 4.2 0.  0 0 1 -3 2 1  5
defaultparticle /Defaults/Particles/bbar
makeanti /Defaults/Particles/bbar /Defaults/Particles/b
create /ThePEG/ParticleData /Defaults/Particles/t
setup /Defaults/Particles/t 6 t 174.2 1.4 14 0 2 3 2 0 
defaultparticle /Defaults/Particles/t
create /ThePEG/ParticleData /Defaults/Particles/tbar
setup /Defaults/Particles/tbar -6 tbar 174.2 1.4 14 0 -2 -3 2 0 
defaultparticle /Defaults/Particles/tbar
makeanti /Defaults/Particles/tbar /Defaults/Particles/t
