###################################################
#
#  Set up spin correlation Decayers
#
###################################################
cd /Herwig/NewPhysics

set TwoBodyDC:CreateDecayModes Yes

###################################################
# Set up the model framework
###################################################
set /Herwig/Feynrules/FRModel:ModelGenerator NewModel
###################################################
#
#  Choose FR over SM
#
###################################################
cd /Herwig/Generators

erase DefaultStrategy:DefaultParticlesDirs[0]
insert DefaultStrategy:DefaultParticlesDirs[0] /Herwig/Feynrules/Particles

set LEPGenerator:StandardModelParameters  /Herwig/Feynrules/FRModel
set LHCGenerator:StandardModelParameters  /Herwig/Feynrules/FRModel
