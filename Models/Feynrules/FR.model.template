###################################################
mkdir /Herwig/Feynrules
cd /Herwig/Feynrules

library FeynrulesModel.so

#### create Herwig::FeynRulesModel FRModel
create Herwig::FeynRulesModel FRModel
# cp /Herwig/Model FRModel

# SM couplings
set FRModel:QCD/RunningAlphaS /Herwig/AlphaS
set FRModel:EW/RunningAlphaEM /Herwig/AlphaEM
set FRModel:EW/CKM /Herwig/CKM
set FRModel:RunningMass /Herwig/RunningMass
###################################################
