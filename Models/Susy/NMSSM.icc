namespace Herwig {

inline NMSSM::NMSSM() {}

inline IBPtr NMSSM::clone() const {
  return new_ptr(*this);
}

inline IBPtr NMSSM::fullclone() const {
  return new_ptr(*this);
}

inline const MixingMatrixPtr & NMSSM::CPoddHiggsMix() const {
