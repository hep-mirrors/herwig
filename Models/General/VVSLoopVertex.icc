// -*- C++ -*-
//
// This is the implementation of the inlined member functions of
// the VVSLoopVertex class.
//

namespace Herwig {
inline VVSLoopVertex::VVSLoopVertex() : theNpart(0) {
  kinematics(true);
}

inline IBPtr VVSLoopVertex::clone() const {
  return new_ptr(*this);
}

inline IBPtr VVSLoopVertex::fullclone() const {
  return new_ptr(*this);
}

inline void VVSLoopVertex::setNParticles(unsigned int npart) {
  theNpart = npart;
}

}
