inline void SemiLeptonicBaryonDecayer::dofinish() {
  DecayIntegrator::dofinish();
}

inline void SemiLeptonicBaryonDecayer::doinitrun() {
  _current->initrun();
  _form->initrun();
  DecayIntegrator::doinitrun();
  _maxwgt.resize(0);
  for(unsigned int ix=0;ix<numberModes();++ix)
    {_maxwgt.push_back(mode(ix)->maxWeight());}
