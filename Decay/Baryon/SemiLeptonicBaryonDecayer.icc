inline SemiLeptonicBaryonDecayer::SemiLeptonicBaryonDecayer() 
{
  // default value of the fermi constant taken from PDG 2002
  _GF = 1.16639E-5/GeV2;
}

inline SemiLeptonicBaryonDecayer::SemiLeptonicBaryonDecayer(const SemiLeptonicBaryonDecayer & x)
  : DecayIntegrator(x), _current(x._current), _form(x._form), _maxwgt(x._maxwgt),
    _modemap(x._modemap), _GF(x._GF) {}

inline IBPtr SemiLeptonicBaryonDecayer::clone() const {
  return new_ptr(*this);
}

inline IBPtr SemiLeptonicBaryonDecayer::fullclone() const {
  return new_ptr(*this);
}

