  : Baryon1MesonDecayerBase(x),_gsigma_clambda_cpi(x._gsigma_clambda_cpi),
    _gxistar_cxi_cpi(x._gxistar_cxi_cpi),_flambda_c1sigma_cpi(x._flambda_c1sigma_cpi),
    _fxi_c1xi_cpi(x._fxi_c1xi_cpi),_flambda_c1starsigma_cpi(x._flambda_c1starsigma_cpi),
    _fxi_c1starxi_cpi(x._fxi_c1starxi_cpi),_gsigma_blambda_bpi(x._gsigma_blambda_bpi),
    _gxistar_bxi_bpi(x._gxistar_bxi_bpi),_flambda_b1sigma_bpi(x._flambda_b1sigma_bpi),
    _fxi_b1xi_bpi(x._fxi_b1xi_bpi),_flambda_b1starsigma_bpi(x._flambda_b1starsigma_bpi),
    _fxi_b1starxi_bpi(x._fxi_b1starxi_bpi),_incoming(x._incoming),
    _outgoingB(x._outgoingB),_outgoingM(x._outgoingM),_maxweight(x._maxweight),
    _prefactor(x._prefactor),_modetype(x._modetype), _initsize(x._initsize) {}

