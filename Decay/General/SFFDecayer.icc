// -*- C++ -*-
//
// This is the implementation of the inlined member functions of
// the SFFDecayer class.
//

namespace Herwig {
  
inline SFFDecayer::SFFDecayer() {
  addToSearchList(2);
}

inline IBPtr SFFDecayer::clone() const {
  return new_ptr(*this);
}

inline IBPtr SFFDecayer::fullclone() const {
  return new_ptr(*this);
}

inline void SFFDecayer::doinit() throw(InitException) {
  _theFFSPtr = dynamic_ptr_cast<FFSVertexPtr>(getVertex());
  GeneralTwoBodyDecayer::doinit();
}

}
