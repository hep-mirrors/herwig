namespace Herwig {


SVVDecayer::SVVDecayer() {
  addToSearchList(2);
}

inline IBPtr SVVDecayer::clone() const {
  return new_ptr(*this);
}

inline IBPtr SVVDecayer::fullclone() const {
  return new_ptr(*this);
}

inline void SVVDecayer::doinit() throw(InitException) {
  GeneralTwoBodyDecayer::doinit();
  _theVVSPtr = dynamic_ptr_cast<VVSVertexPtr>(getVertex());
}

}
